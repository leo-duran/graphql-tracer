{
  "name": "graphql-tracer",
  "version": "0.2.5",
  "description": "Trace collection library for Node.js GraphQL servers",
  "main": "dist/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "istanbul cover ./node_modules/mocha/bin/_mocha -- --reporter spec --full-trace test/index.js",
    "lint": "eslint .",
    "testonly": "mocha test/index.js",
    "prepublish": "babel ./src --ignore test --out-dir ./dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apollostack/tracer.git"
  },
  "keywords": [
    "GraphQL",
    "Apollo",
    "Javascript",
    "Trace"
  ],
  "author": "Jonas Helfer <helfer@meteor.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/apollostack/tracer/issues"
  },
  "homepage": "https://github.com/apollostack/tracer#readme",
  "dependencies": {
    "node-uuid": "^1.4.7",
    "performance-now": "^0.2.0",
    "request": "^2.72.0"
  },
  "peerDependencies": {
    "graphql-tools": "^0.6.3"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "6.14.0",
    "babel-eslint": "^6.0.0-beta.6",
    "babel-loader": "6.2.5",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-plugin-import": "^1.1.0",
    "graphql": "^0.7.0",
    "graphql-tools": "^0.6.5",
    "istanbul": "1.0.0-alpha.2",
    "mocha": "^3.0.2"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "airbnb/base",
      "plugin:import/errors"
    ],
    "rules": {
      "no-use-before-define": 0,
      "arrow-body-style": 0,
      "dot-notation": 0,
      "no-console": 0
    },
    "env": {
      "mocha": true
    }
  }
}
